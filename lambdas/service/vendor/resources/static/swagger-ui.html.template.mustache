<!-- HTML for static distribution bundle build -->
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>Swagger UI</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/4.15.2/swagger-ui.css" integrity="sha384-3PETiwxSbPbIe38ec8dFq4c05S6yEg3h3b+kRKTmXIaYZEY7OOLNvzKTDzb6xF+U" crossorigin="anonymous">
    <style>
      html
      {
        box-sizing: border-box;
        overflow: -moz-scrollbars-vertical;
        overflow-y: scroll;
      }
      *,
      *:before,
      *:after
      {
        box-sizing: inherit;
      }
      body
      {
        margin:0;
        background: #fafafa;
      }
    </style>
  </head>

  <body>
    <div id="swagger-ui"></div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/4.15.2/swagger-ui-bundle.js" integrity="sha384-l16FNZuUX/wtE0D5emp+JcKr8opMlUv9iX9pMMER/Qah5edBsgPJ/xJzRBu2k62x" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/4.15.2/swagger-ui-standalone-preset.js" integrity="sha384-LPIWB4adMa/c1eVa/Jc8ShfB3GG4sxupb/nSFLbjiIhnM78eY2zgZImCSGhpdBJA" crossorigin="anonymous"></script>
    <script>
    window.onload = function() {
      // Adapted from https://github.com/swagger-api/swagger-ui/issues/3725#issuecomment-334899276
      const DisableTryItOutPlugin = function() {
        return {
          statePlugins: {
            spec: {
              wrapSelectors: {
                allowTryItOutFor: (oriSelector, system) => (state, ...args) => {
                  return oriSelector(state, ...args) && ({{{NON_INTERACTIVE_METHODS}}}.indexOf(args.join('/')) == -1);
                }
              }
            }
          }
        }
      }
      // Begin Swagger UI call region
      const ui = SwaggerUIBundle({
        url: {{{DEPLOYMENT_PATH}}},
        dom_id: '#swagger-ui',
        deepLinking: true,
        presets: [
          SwaggerUIBundle.presets.apis,
        ],
        plugins: [
          SwaggerUIBundle.plugins.DownloadUrl,
          DisableTryItOutPlugin
        ],
        oauth2RedirectUrl: {{{OAUTH2_REDIRECT_URL}}}
      })
      const client_id = {{{OAUTH2_CLIENT_ID}}};
      if (client_id !== null) {
          ui.initOAuth({
            clientId: client_id
          })
      }
      // End Swagger UI call region
      window.ui = ui
    }
  </script>
  </body>
</html>